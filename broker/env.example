# MQTT Broker Configuration
MQTT_HOST=localhost
MQTT_PORT=1883
WS_PORT=9090
WEB_PORT=3001

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Authentication
REQUIRE_AUTH=false
MQTT_USERNAME=admin
MQTT_PASSWORD=password123

# Broker Settings
BROKER_ID=mqtt-broker-001
LOG_LEVEL=info

# Connection Settings
MAX_CONNECTIONS=1000
KEEP_ALIVE=60
ALLOW_ANONYMOUS=true

# Rate Limiting
RATE_LIMIT_ENABLED=false
RATE_LIMIT_MAX_MESSAGES=100
RATE_LIMIT_WINDOW_MS=60000

# Environment
NODE_ENV=development
